<!DOCTYPE html>
<html>
<head>
  <title>Lista de Usuários</title>
</head>
<body>
  <h1>Lista de Usuários</h1>
  <ul>
    <% users.forEach(user => { %>
      <li>
        <img src="<%= user.imageId %>" alt="Imagem do usuário" width="50" height="50">
        <strong><%= user.name %></strong> (<%= user.role %>)
        <ul>
          <% user.modules.forEach(module => { %>
            <li><%= module.name %></li>
          <% }) %>
        </ul>
      </li>
    <% }) %>
  </ul>
</body>
</html>